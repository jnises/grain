{
  "name": "grain",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:stop": "node scripts/stop-dev-server.js",
    "dev:restart": "npm run dev:stop && npm run dev",
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "check": "npm run lint && npm run type-check",
    "preview": "vite preview",
    "test:watch": "vitest",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "benchmark": "vitest run --config vitest.benchmark.config.ts",
    "profile": "npx tsx scripts/profiling/flamegraph_profiler.ts",
    "profile:comprehensive": "scripts/profiling/profile_hotspots.sh"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
